  "packageManager": "yarn@3.3.1",
  "scripts": {
    "build": "yarn del-cli dist --force  && yarn run webpack --mode production --env production",
    "code-style-check": "yarn run lint-check && yarn run prettier:check",
    "dev": "yarn run webpack-dev-server --progress --mode development --env development",
    "lint-check": "yarn eslint ./src/**/*.ts",
    "prettier": "npx prettier",
    "prettier:check": "yarn run prettier -- -c ./src/**/*.ts",
    "prettier:format": "yarn run prettier -- --write src/**/*.ts",
    "start": "node ./server.js",
    "test": "yarn cypress run --spec 'cypress/e2e/pnp/**/*.ts'",
    "cy:run": "yarn cypress run",
    "test-parallel": "cypress-parallel -s 'cy:run' -t 6 -d 'cypress/e2e/pnp/parallelizable/*.ts' && cypress-parallel -s 'cy:run' -t 1 -d 'cypress/e2e/pnp/non-parallelizable/*.ts'",
    "test-debug": "DEBUG='cypress:*' yarn cypress run --spec 'cypress/e2e/pnp/*.ts'",
    "test-editor": "yarn cypress open",
    "webpack": "webpack",
    "webpack-dev-server": "webpack serve --hot --open"
  },
  "version": "0.1.0"
}
