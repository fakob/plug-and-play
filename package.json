{
  "name": "pixi-typescript-boilerplate",
  "version": "3.0.0",
  "description": "pixi.js typescript boilerplate webpack",
  "browserslist": "> 0.25%, not dead",
  "scripts": {
    "build": "yarn del-cli dist --force  && yarn run webpack --mode production --env production",
    "code-coverage": "jest --coverage",
    "code-style-check": "yarn run lint-check && yarn run prettier:check",
    "dev": "yarn run webpack-dev-server --progress --mode development --env development",
    "lint-check": "yarn eslint ./src/**/*.ts",
    "prettier": "npx prettier",
    "prettier:check": "yarn run prettier -- -c ./src/**/*.ts",
    "prettier:format": "yarn run prettier -- --write src/**/*.ts",
    "start": "node ./server.js",
    "test": "yarn jest",
    "webpack": "webpack",
    "webpack-dev-server": "webpack serve --hot --open",
    "prepare": "husky install"
  },
  "author": "jkanchelov",
  "license": "MIT",
  "dependencies": {
    "@blueprintjs/core": "^3.49.1",
    "@blueprintjs/popover2": "^0.11.4",
    "@blueprintjs/select": "^3.18.1",
    "@blueprintjs/table": "^3.9.3",
    "@pixi/filter-drop-shadow": "^4.1.3",
    "@pixi/text-html": "^2.0.0",
    "color": "^4.0.1",
    "core-js": "^3.17.2",
    "dat.gui": "^0.7.7",
    "dexie": "^3.0.3",
    "eslint-webpack-plugin": "^3.0.1",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "human-readable-ids": "^1.0.4",
    "javascript-time-ago": "^2.3.8",
    "monaco-editor-webpack-plugin": "^4.1.2",
    "papaparse": "^5.3.1",
    "pixi-viewport": "^4.32.0",
    "pixi.js": "^6.1.2",
    "pretty-format": "^27.1.0",
    "re-resizable": "^6.9.0",
    "react": "^17.0.1",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.3.4",
    "react-error-boundary": "^3.1.3",
    "react-monaco-editor": "^0.44.0",
    "slate": "^0.65.3",
    "slate-history": "^0.65.3",
    "slate-react": "^0.65.3",
    "strip-comments": "^2.0.1",
    "to-json-schema": "^0.2.5",
    "util": "^0.12.4"
  },
  "devDependencies": {
    "@babel/core": "^7.15.5",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.4",
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-env": "^7.15.4",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/runtime": "^7.15.4",
    "@types/copy-webpack-plugin": "^8.0.1",
    "@types/html-webpack-plugin": "^3.2.6",
    "@types/jest": "^27.0.1",
    "@types/mini-css-extract-plugin": "^2.2.0",
    "@types/node": "^16.7.10",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.4",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@types/terser-webpack-plugin": "^5.2.0",
    "@types/webpack": "^5.28.0",
    "@types/webpack-dev-server": "^4.1.0",
    "@types/webpack-merge": "^5.0.0",
    "@typescript-eslint/eslint-plugin": "^4.30.0",
    "@typescript-eslint/parser": "^4.30.0",
    "babel-loader": "^8.1.0",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "9.0.1",
    "css-loader": "^6.2.0",
    "cypress": "^8.3.1",
    "del-cli": "^4.0.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "html-webpack-plugin": "^5.3.2",
    "husky": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.1.0",
    "jest-canvas-mock": "^2.2.0",
    "jsdom": "^17.0.0",
    "mini-css-extract-plugin": "^2.2.2",
    "optimize-css-assets-webpack-plugin": "^6.0.1",
    "prettier": "^2.3.2",
    "pretty-quick": "^3.1.1",
    "terser-webpack-plugin": "^5.2.3",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2",
    "typescript-plugin-css-modules": "^3.4.0",
    "webpack": "^5.52.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.1.0",
    "webpack-merge": "^5.8.0"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "jest-canvas-mock"
    ]
  },
  "engines": {
    "node": "14.x"
  }
}
