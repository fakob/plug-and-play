    "@typescript-eslint/parser": "^5.36.2",
    "babel-loader": "^8.2.3",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "9.0.1",
    "css-loader": "^6.5.1",
    "cypress": "^10.7.0",
    "del-cli": "^5.0.0",
