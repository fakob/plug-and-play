{
  "name": "pixi-typescript-boilerplate",
  "version": "3.0.0",
  "description": "pixi.js typescript boilerplate webpack",
  "browserslist": "> 0.25%, not dead",
  "scripts": {
    "build": "yarn del-cli dist --force  && yarn run webpack --mode production --env production",
    "code-coverage": "jest --coverage",
    "code-style-check": "yarn run lint-check && yarn run prettier:check",
    "dev": "yarn run webpack-dev-server --progress --mode development --env development",
    "lint-check": "yarn eslint ./src/**/*.ts",
    "prettier": "npx prettier",
    "prettier:check": "yarn run prettier -- -c ./src/**/*.ts",
    "prettier:format": "yarn run prettier -- --write src/**/*.ts",
    "start": "node ./server.js",
    "test": "yarn jest",
    "webpack": "webpack",
    "webpack-dev-server": "webpack serve --hot --open"
  },
  "author": "jkanchelov",
  "license": "MIT",
  "dependencies": {
    "@blueprintjs/core": "^3.38.2",
    "@blueprintjs/popover2": "^0.3.1",
    "@blueprintjs/select": "^3.15.4",
    "@blueprintjs/table": "^3.8.26",
    "@pixi/filter-drop-shadow": "^4.1.0",
    "@pixi/text-html": "^1.0.0",
    "axios": "^0.21.0",
    "color": "^3.1.3",
    "core-js": "^3.6.5",
    "dat.gui": "^0.7.7",
    "dexie": "^3.0.3",
    "eslint-webpack-plugin": "^2.4.3",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "human-readable-ids": "^1.0.4",
    "monaco-editor-webpack-plugin": "^3.0.0",
    "papaparse": "^5.3.0",
    "pixi-viewport": "^4.31.0",
    "pixi.js": "^6.0.4",
    "pretty-format": "^26.6.2",
    "re-resizable": "^6.9.0",
    "react": "^17.0.1",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.3.1",
    "react-monaco-editor": "^0.42.0",
    "slate": "^0.62.0",
    "slate-history": "^0.62.0",
    "slate-react": "^0.62.0",
    "strip-comments": "^2.0.1",
    "to-json-schema": "^0.2.5",
    "util": "^0.12.3"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-decorators": "^7.10.5",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@types/copy-webpack-plugin": "^6.0.0",
    "@types/html-webpack-plugin": "^3.2.3",
    "@types/jest": "^26.0.14",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/node": "^14.11.2",
    "@types/optimize-css-assets-webpack-plugin": "^5.0.1",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/terser-webpack-plugin": "^5.0.2",
    "@types/webpack": "^4.41.22",
    "@types/webpack-dev-server": "^3.11.0",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "babel-loader": "^8.1.0",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "6.4.1",
    "css-loader": "^5.1.0",
    "cypress": "^6.6.0",
    "del-cli": "^3.0.1",
    "eslint": "^7.9.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "html-webpack-plugin": "^5.2.0",
    "husky": "4.3.8",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-canvas-mock": "^2.2.0",
    "jsdom": "^16.4.0",
    "mini-css-extract-plugin": "^1.3.9",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "terser-webpack-plugin": "^5.1.1",
    "ts-jest": "^26.4.0",
    "ts-loader": "^8.0.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "typescript-plugin-css-modules": "^3.1.0",
    "webpack": "^5.24.2",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.1.4"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      ".+\\.(css|styl|less|sass|scss|png|jpg|ttf|woff|woff2)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "jest-canvas-mock"
    ]
  },
  "engines": {
    "node": "12.x"
  }
}
